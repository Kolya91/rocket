{
  "name": "rocket",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cc": "jest --coverage",
    "lint": "ng lint",
    "lint:styles": "stylelint **/*.scss",
    "e2e": "ng e2e"
  },
  "private": true,
  "workspaces": [
    "src/*"
  ],
  "jest": {
    "preset": "jest-preset-angular",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "src/tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(ramda/es|@ngrx))"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.ts"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/jest.ts",
      "!src/polyfills.ts",
      "!src/**/index.ts",
      "!src/**/main.ts",
      "!src/typings.d.ts",
      "!src/environments/**",
      "!**/node_modules/**",
      "!src/**/*.constants.ts",
      "!src/**/*.settings.ts",
      "!src/**/*.mocks.ts",
      "!src/**/*.module.ts",
      "!src/**/*.model.ts",
      "!src/**/*.enum.ts",
      "!src/**/*.routes.ts"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@root/(.*)": "<rootDir>/src/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint && npm run lint:styles",
      "pre-push": "jest"
    }
  },
  "dependencies": {
    "@angular/animations": "~9.1.2",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~9.1.2",
    "@angular/compiler": "~9.1.2",
    "@angular/core": "~9.1.2",
    "@angular/forms": "~9.1.2",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "~9.1.2",
    "@angular/platform-browser-dynamic": "~9.1.2",
    "@angular/router": "~9.1.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "~9.1.2",
    "@angular/compiler-cli": "~9.1.2",
    "@angular/language-service": "~9.1.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "codeceptjs": "^2.6.2",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "jest-preset-angular": "^8.1.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "puppeteer": "^3.0.1",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  }
}
